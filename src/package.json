    "npm": "^5.8.0",
    "passport": "^0.4.0",
    "passport-vkontakte": "^0.3.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
