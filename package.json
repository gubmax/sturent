{
  "name": "sturent",
  "version": "1.0.0",
  "description": "sturent",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "npm run dev:webpack && npm run start:server",
    "watch": "npm run watch:webpack & npm run watch:server",
    "start:server": "babel-node ./src/server/app.js",
    "watch:server": "nodemon ./src/server/app.js localhost 8000 --exec babel-node --delay 1",
    "dev:webpack": "rm -rf ./src/public/js/* && webpack -d --colors --progress --display-modules",
    "watch:webpack": "rm -rf ./src/public/js/* && webpack -d --watch --colors --progress --display-modules",
    "build": "rm -rf ./src/public/js/* && NODE_ENV='production' webpack -p --progress --colors",
    "mongod": "rm -rf ./mongodb/* && mongod --config ./mongod.conf",
    "lint": "eslint src/ --ext .js,.jsx --fix; exit 0",
    "stylelint": "stylelint '**/*.css' --fix; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gubmax/sturent.git"
  },
  "author": {
    "name": "Gubyshev M.A.",
    "email": "gubyshev@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.4",
    "install": "^0.12.2",
    "isomorphic-style-loader": "^4.0.0",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.3.4",
    "passport": "^0.4.0",
    "passport-vkontakte": "^0.3.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.3",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.1.2",
    "stylelint": "^9.6.0",
    "stylelint-config-airbnb": "0.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
