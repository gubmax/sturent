{
    "css": {
        "allowed_file_extensions": ["css"],
        "end_with_newline": true,
        "indent_char": " ",
        "indent_size": 4,
        "newline_between_rules": false,
        "selector_separator": " ",
        "selector_separator_newline": true,
    },
    "js": {
        "allowed_file_extensions": ["js", "jsx", "json", "jsbeautifyrc"],
        "indent_level": 0,
        "indent_size": 4,
        "indent_with_tabs": false,
        "jslint_happy": true,
    }
}
